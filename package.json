{
  "name": "spoker",
  "version": "1.16.10",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "npx turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "check:turbo": "npx turbo lint type-check",
    "format": "prettier --write src",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin dev",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "generate-rules": "ts-node tools/generateRules.ts",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/gabarito": "^5.0.1",
    "@fontsource/gantari": "^5.0.8",
    "@hookform/resolvers": "^3.3.2",
    "@sentry/nextjs": "^7.80.0",
    "firebase": "^10.6.0",
    "framer-motion": "^10.16.4",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.3",
    "next": "^14.0.2",
    "next-seo": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.48.2",
    "react-icons": "^4.11.0",
    "react-rewards": "^2.0.4",
    "react-sortablejs": "^6.1.4",
    "react-textarea-autosize": "^8.5.3",
    "sortablejs": "^1.15.0",
    "twemoji": "14.0.2",
    "valibot": "^0.20.1",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.4.0",
    "@commitlint/cz-commitlint": "^18.4.0",
    "@jahed/firebase-rules": "^1.1.0",
    "@types/lodash-es": "^4.17.11",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/sortablejs": "^1.15.5",
    "all-contributors-cli": "^6.26.1",
    "babel-loader": "^9.1.3",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "eslint-config-sznm": "^2.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "url": "https://github.com/sozonome/spoker"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  },
  "engines": {
    "node": ">=16.8.x",
    "pnpm": ">=8"
  }
}
